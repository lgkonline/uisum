{
    "name": "uisum",
    "version": "4.4.1",
    "description": "UI library for/with React",
    "main": "index.js",
    "scripts": {
        "test": "jest",
        "doc-build": "webpack -p",
        "build": "babel ./src --out-dir ./dist & node-sass ./dist/style.scss ./dist/style.css",
        "doc": "react-docgen ./src -o ./docs/src/data/doc.json && node ./followUpDoc.js && node ./convertScssVarsToJson.js",
        "doc-doc": "react-docgen ./docs/src -o ./docs/src/data/doc-doc.json",
        "watch": "webpack -d --watch",
        "put-version": "put-version ./docs/index.html",
        "build-all": "npm run doc && npm run doc-doc && npm run build && npm run doc-build && npm run put-version",
        "publish": "npm run build-all",
        "prepublishOnly": "node ./prepublish.js",
        "postpublish": "node ./postpublish.js",
        "serve": "http-server ./docs -o",
        "start": "npm run serve | npm run watch",
        "start-no-open": "http-server ./docs | npm run watch"
    },
    "author": "Lars / LGK",
    "license": "MIT",
    "devDependencies": {
        "babel-cli": "^6.24.1",
        "babel-core": "^6.24.0",
        "babel-jest": "^23.0.1",
        "babel-loader": "^7.0.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-es2015": "^6.24.0",
        "babel-preset-react": "^6.24.1",
        "css-loader": "^0.28.1",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^1.1.5",
        "http-server": "^0.11.1",
        "jest": "^23.1.0",
        "jsdom": "^11.1.0",
        "json-loader": "^0.5.7",
        "lgk": "^1.2.2",
        "node-sass": "^4.5.2",
        "opn-cli": "^3.1.0",
        "prop-types": "^15.5.10",
        "put-version": "^1.0.9",
        "react-docgen": "^2.17.0",
        "react-dom": "^16.1.1",
        "react-highlight": "github:briancappello/react-highlight#react-v16-compiled",
        "react-test-renderer": "^16.4.0",
        "reactstrap": "^6.1.0",
        "sass-loader": "^6.0.4",
        "superagent": "^3.5.2",
        "webpack": "^3.8.1"
    },
    "dependencies": {
        "bootstrap": "^4.0.0",
        "custom-event-polyfill": "^0.3.0",
        "react": "^16.1.1"
    }
}